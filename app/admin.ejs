        <nav>
          <ul class="list-unstyled">
            <li class="pos-relative mb-3 bg-gray-400-hover">
              <a href="#" class="link link--block d-flex justify-content-start align-items-center">
                <span class="ml-4 material-icons p-1 mr-2 text-gray-900">assessment</span>
                <span class="fw-bold align-middle text-gray-900">Dashboard</span>
              </a>
            </li>
            <li class="pos-relative mb-3 bg-gray-400-hover">
              <a href="#" class="link link--block d-flex justify-content-start align-items-center">
